<block>
  <name>AM FSK Modulator</name>
  <key>am_fsk_mod_bc</key>
  <category>[Radioteletype]</category>
  <import>from radioteletype.modulators import am_fsk_mod_bc</import>
  <make>am_fsk_mod_bc(
    spacing=$spacing,
    samp_per_bit=$samp_per_bit,
    samp_rate=$samp_rate,
    taps=$taps,
)</make>
  <callback>set_spacing($spacing)</callback>
  <callback>set_samp_per_bit($samp_per_bit)</callback>
  <callback>set_samp_rate($samp_rate)</callback>
  <callback>set_taps($taps)</callback>
  <param>
    <name>Spacing</name>
    <key>spacing</key>
    <value>170</value>
    <type>raw</type>
  </param>
  <param>
    <name>Samples per Bit</name>
    <key>samp_per_bit</key>
    <value>20</value>
    <type>raw</type>
  </param>
  <param>
    <name>Sample Rate</name>
    <key>samp_rate</key>
    <value>1000</value>
    <type>raw</type>
  </param>
  <param>
    <name>Taps</name>
    <key>taps</key>
    <value>None</value>
    <type>raw</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
    <vlen>1</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>1</vlen>
  </source>
</block>
